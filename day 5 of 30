#include <iostream>
using namespace std;
int main()
{
int n=10;
int first = 0 , second = 1 , next;
    cout<<"first 10 numbers of fibonacci series:"<<endl;
    for(int i =0;i<n;i++)
  {
        if(i<=1)
    {
        next=i+1;
    }
    else
      {
        next=first+second;
        first = second;
        second = next;
      }
    cout<<next<<" ";
  }

 
return 0;
}

